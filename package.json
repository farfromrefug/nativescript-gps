{
    "name": "nativescript-gps",
    "version": "1.0.0",
    "description": "Track GPS location updates",
    "main": "src/nativescript-gps.js",
    "typings": "src/nativescript-gps.d.ts",
    "nativescript": {
        "platforms": {
            "android": "2.3.0",
            "ios": "2.3.0"
        }
    },
    "scripts": {
        "tsc": "cpy '**/*.d.ts' '../plugin' --parents --cwd=src && cp README.md plugin/ && tsc -skipLibCheck",
        "demo.ios": "npm run preparedemo && cd demo && tns emulate ios",
        "demo.ios.device": "npm run preparedemo && cd demo && tns run ios",
        "demo.android": "npm run preparedemo && cd demo && tns run android",
        "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-gps && tns plugin add .. && tns install",
        "setup": "npm i && cd demo && npm i && cd .. && npm run build && cd demo && tns plugin add .. && cd .."
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/farfromrefug/nativescript-gps.git"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
    "author": {
        "name": "Martin Guillon",
        "email": "martin.guillon@akylas.fr"
    },
    "bugs": {
        "url": "https://github.com/farfromrefug/nativescript-gps/issues"
    },
    "license": "MIT",
    "homepage": "https://github.com/farfromrefug/nativescript-gps",
    "readmeFilename": "../README.md",
    "devDependencies": {
        "@nativescript/core": "rc",
        "cpy-cli": "^3.0.0",
        "nativescript-observable": "^2.0.1",
        "nativescript-perms": "^2.0.1",
        "tns-platform-declarations": "rc",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^9.0.0",
        "typescript": "~3.7.3",
        "prompt": "^1.0.0",
        "rimraf": "^3.0.0"
    }
}
